{
  "name": "studio-roster",
  "version": "0.0.1",
  "description": "AI-Native Agency Management System",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "nest build -p apps/api/tsconfig.app.json && vite build",
    "build:api": "nest build -p apps/api/tsconfig.app.json",
    "start": "node build/apps/api/src/main",
    "start:dev": "nest start --watch",
    "start:prod": "node build/apps/api/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --config apps/api/jest.config.ts",
    "test:watch": "jest --config apps/api/jest.config.ts --watch",
    "test:cov": "jest --config apps/api/jest.config.ts --coverage",
    "test:e2e": "jest --config apps/api/jest.config.ts --testRegex '.e2e-spec.ts$'",
    "test:integration": "jest --config apps/api/jest.config.ts --testRegex '.integration.spec.ts$'",
    "test:frontend": "vitest run",
    "test:frontend:ui": "vitest --ui"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^5.13.0",
    "@google-cloud/storage": "^7.17.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^7.0.1",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "buffer": "^6.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "exceljs": "^4.3.0",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.0",
    "lucide-react": "^0.260.0",
    "mammoth": "^1.11.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "prisma": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.67.0",
    "react-router-dom": "^6.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.18.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.20",
    "@prisma/config": "^7.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.27",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.4.5",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^4.4.0",
    "vitest": "^4.0.14"
  }
}
