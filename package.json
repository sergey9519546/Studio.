{
  "name": "studio-roster",
  "version": "0.0.1",
  "description": "AI-Native Agency Management System",
  "private": true,
  "scripts": {
    "dev": "nx run-many -t serve",
    "prepare": "prisma generate",
    "build": "npm run build:client && npm run build:api",
    "build:client": "vite build",
    "build:frontend": "vite build",
    "build:api": "npx tsc --project apps/api/tsconfig.app.json --outDir build/apps/api",
    "gcp-build": "npm run build",
    "start": "node build/apps/api/src/main.js",
    "start:dev": "nx serve api",
    "start:prod": "node build/apps/api/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "test": "prisma generate && jest --config apps/api/jest.config.ts --max-workers=2",
    "test:watch": "jest --config apps/api/jest.config.ts --watch",
    "test:cov": "jest --config apps/api/jest.config.ts --coverage",
    "test:e2e": "jest --config apps/api/jest.config.ts --testRegex '.e2e-spec.ts$'",
    "test:integration": "jest --config apps/api/jest.config.ts --testRegex '.integration.spec.ts$' --passWithNoTests",
    "test:frontend": "vitest run",
    "test:frontend:ui": "vitest --ui",
    "verify-deployment": "node scripts/verify-deployment.js"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@google-cloud/storage": "^7.17.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.1.12",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.12",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/adapter-pg": "7.1.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@tanstack/react-query": "^5.90.12",
    "aria-query": "^5.1.3",
    "better-sqlite3": "^12.6.2",
    "buffer": "^6.0.3",
    "cache-manager": "^7.2.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.0.1",
    "framer-motion": "^12.23.25",
    "helmet": "^8.1.0",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.260.0",
    "nestjs-pino": "^4.5.0",
    "pg": "^8.16.3",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.67.0",
    "react-router-dom": "^6.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "unsplash-js": "^7.0.20",
    "winston": "^3.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ariakit/react": "^0.4.20",
