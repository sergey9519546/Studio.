    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "vitest-axe": "^0.1.0"
  },
  "overrides": {
    "@nestjs/cli": {
      "glob": "10.1.0"
    },
    "@angular-devkit/schematics-cli": {
      "glob": "10.1.0"
    },
    "webpack": "5.94.0",
    "tmp": "0.2.4",
    "hono": "4.11.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
