    "autoprefixer": "^10.4.14",
    "axe-core": "^4.11.0",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "glob": "10.1.0",
    "globals": "^16.5.0",
    "inquirer": "^13.1.0",
